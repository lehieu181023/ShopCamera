@{
    ViewData["Title"] = "Bán chạy";
    ViewData["menuSelect"] = "banchay";
}
@using System.Globalization
@{
    var BestSeller = ViewData["BestSellers"] as List<CameraTuanA.Models.Product>;
    var NewProducts = ViewData["NewProducts"] as List<CameraTuanA.Models.Product>;
    var AllProducts = ViewData["AllProducts"] as List<CameraTuanA.Models.Product>;
}
<!-- Single Page Header start -->
<div class="container-fluid page-header py-5">
    <h1 class="text-center text-white display-6 wow fadeInUp" data-wow-delay="0.1s">Sản phẩm bán chạy</h1>
    <ol class="breadcrumb justify-content-center mb-0 wow fadeInUp" data-wow-delay="0.3s">
        <li class="breadcrumb-item"><a >Trang chủ</a></li>
        <li class="breadcrumb-item"><a >Trang</a></li>
        <li class="breadcrumb-item active text-white">Bán chạy</li>
    </ol>
</div>
<!-- Single Page Header End -->
<!-- Searvices Start -->
<div class="container-fluid px-0">
    <div class="row g-0">
        <div class="col-6 col-md-4 col-lg-2 border-start border-end wow fadeInUp" data-wow-delay="0.1s">
            <div class="p-4">
                <div class="d-inline-flex align-items-center">
                    <i class="fa fa-sync-alt fa-2x text-primary"></i>
                    <div class="ms-4">
                        <h6 class="text-uppercase mb-2">Miễn phí đổi trả</h6>
                        <p class="mb-0">Đảm bảo hoàn tiền trong vòng 30 ngày!</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-6 col-md-4 col-lg-2 border-end wow fadeInUp" data-wow-delay="0.2s">
            <div class="p-4">
                <div class="d-flex align-items-center">
                    <i class="fab fa-telegram-plane fa-2x text-primary"></i>
                    <div class="ms-4">
                        <h6 class="text-uppercase mb-2">Miễn phí vẫn chuyển</h6>
                        <p class="mb-0">Miễn phí vận chuyển cho tất cả đơn hàng</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-6 col-md-4 col-lg-2 border-end wow fadeInUp" data-wow-delay="0.3s">
            <div class="p-4">
                <div class="d-flex align-items-center">
                    <i class="fas fa-life-ring fa-2x text-primary"></i>
                    <div class="ms-4">
                        <h6 class="text-uppercase mb-2">Hỗ trợ 24/7</h6>
                        <p class="mb-0">Chúng tôi hỗ trợ trực tuyến 24 giờ một ngày</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-6 col-md-4 col-lg-2 border-end wow fadeInUp" data-wow-delay="0.4s">
            <div class="p-4">
                <div class="d-flex align-items-center">
                    <i class="fas fa-credit-card fa-2x text-primary"></i>
                    <div class="ms-4">
                        <h6 class="text-uppercase mb-2">Nhận thẻ quà</h6>
                        <p class="mb-0">Nhận quà khi đơn hàng trên 5.000.000</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-6 col-md-4 col-lg-2 border-end wow fadeInUp" data-wow-delay="0.5s">
            <div class="p-4">
                <div class="d-flex align-items-center">
                    <i class="fas fa-lock fa-2x text-primary"></i>
                    <div class="ms-4">
                        <h6 class="text-uppercase mb-2">Thanh toán an toàn</h6>
                        <p class="mb-0">Chúng tôi coi trọng sự an toàn của bạn</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-6 col-md-4 col-lg-2 border-end wow fadeInUp" data-wow-delay="0.6s">
            <div class="p-4">
                <div class="d-flex align-items-center">
                    <i class="fas fa-blog fa-2x text-primary"></i>
                    <div class="ms-4">
                        <h6 class="text-uppercase mb-2">Dịch vụ trực tuyến</h6>
                        <p class="mb-0">Sản phẩm có thể trả lại miễn phí trong 30 ngày</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Searvices End -->
<!-- Bestseller Products Start -->
<div class="container-fluid products pt-5">
    <div class="container products-mini py-5">
        <div class="mx-auto text-center mb-5" style="max-width: 700px;">
            <h4 class="text-primary mb-4 border-bottom border-primary border-2 d-inline-block p-2 title-border-radius wow fadeInUp"
                data-wow-delay="0.1s">
                Sản phẩm bán chạy
            </h4>
            <p class="mb-0 wow fadeInUp" data-wow-delay="0.2s">Bạn muốn tìm kiếm những sản phẩm bán chạy nhất của chúng tôi hãy bấm ngay vào mua hàng chúng tôi có những sản phẩm bán chạy nhất để bạn dễ dàng lựa chọn.</p>
        </div>
        <div class="row g-4">
            @foreach (var item in (BestSeller ?? Enumerable.Empty<Product>()).Take(6))
            {
                <div class="col-md-6 col-lg-6 col-xl-4 wow fadeInUp" data-wow-delay="0.1s">
                    <div class="products-mini-item border">
                        <div class="row g-0">
                            <div class="col-5">
                                <div class="products-mini-img border-end h-100">
                                    <img src="@item.MainImage" class="img-fluid w-100 h-100" alt="@item.ProductName">
                                    <div class="products-mini-icon rounded-circle bg-primary">
                                        <a href="@Url.Action("SingleProduct", "Product", new { Id = item.Id })"><i class="fa fa-eye fa-1x text-white"></i></a>
                                    </div>
                                </div>
                            </div>
                            <div class="col-7">
                                <div class="products-mini-content p-3">
                                    <a class="d-block mb-2">Camera</a>
                                    <a href="/SingelProduct/@item.Id" class="d-block h4">@item.ProductName</a>
                                    <del class="me-2 fs-5">@String.Format(new CultureInfo("vi-VN"), "{0:C0}", item.OriginalPrice)</del>
                                    <span class="text-primary fs-5">@String.Format(new CultureInfo("vi-VN"), "{0:C0}", item.SellingPrice)</span>
                                </div>
                            </div>
                        </div>
                        <div class="products-mini-add border p-3">
                            <a onclick="addToCart(@item.Id)" class="btn btn-primary border-secondary rounded-pill py-2 px-4">
                                <i class="fas fa-shopping-cart me-2"></i> Add To Cart
                            </a>
                        </div>
                    </div>
                </div>
            }
            

        </div>
    </div>
</div>
<!-- Bestseller Products End -->
<!-- Our Products Start -->
<div class="container-fluid product pt-5">
    <div class="container py-5">
        <div class="tab-class">
            <div class="row g-4">
                <div class="col-lg-4 text-start wow fadeInLeft" data-wow-delay="0.1s">
                    <h1>Sản phẩm</h1>
                </div>
                <div class="col-lg-8 text-end wow fadeInRight" data-wow-delay="0.1s">
                    <ul class="nav nav-pills d-inline-flex text-center mb-5">
                        <li class="nav-item mb-4">
                            <a class="d-flex mx-2 py-2 bg-light rounded-pill active" data-bs-toggle="pill"
                               href="#tab-1">
                                <span class="text-dark" style="width: 130px;">Tất cả sản phẩm</span>
                            </a>
                        </li>
                        <li class="nav-item mb-4">
                            <a class="d-flex py-2 mx-2 bg-light rounded-pill" data-bs-toggle="pill" href="#tab-2">
                                <span class="text-dark" style="width: 130px;">Hàng mới về</span>
                            </a>
                        </li>
                        <li class="nav-item mb-4">
                            <a class="d-flex mx-2 py-2 bg-light rounded-pill" data-bs-toggle="pill" href="#tab-3">
                                <span class="text-dark" style="width: 130px;">Bán chạy nhất</span>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="tab-content">
                <div id="tab-1" class="tab-pane fade show p-0 active">
                    <div class="row g-4">
                        @foreach (var item in (AllProducts ?? Enumerable.Empty<Product>()).Take(8))
                        {
                            <div class="col-md-6 col-lg-4 col-xl-3">
                                <div class="product-item rounded wow fadeInUp" data-wow-delay="0.1s">
                                    <div class="product-item-inner border rounded">
                                        <div class="product-item-inner-item">
                                            <img src="@item.MainImage" class="img-fluid w-100 rounded-top" alt="">
                                            @if (item.CreatedAt >= DateTime.Now.AddMonths(-1))
                                            {
                                                <div class="product-new">New</div>
                                            }
                                            <div class="product-details">
                                                <a href="/SingleProduct/@item.Id"><i class="fa fa-eye fa-1x"></i></a>
                                            </div>
                                        </div>
                                        <div class="text-center rounded-bottom p-4">
                                            <a class="d-block mb-2">Camera</a>
                                            <a href="/SingleProduct/@item.Id" class="d-block h4">@item.ProductName</a>
                                            <del class="me-2 fs-5">@String.Format(new CultureInfo("vi-VN"), "{0:C0}", item.OriginalPrice)</del>
                                            <span class="text-primary fs-5">@String.Format(new CultureInfo("vi-VN"), "{0:C0}", item.SellingPrice)</span>
                                        </div>
                                    </div>
                                    <div class="product-item-add border border-top-0 rounded-bottom  text-center p-4 pt-0">
                                        <a onclick="addToCart(@item.Id)"
                                           class="btn btn-primary border-secondary rounded-pill py-2 px-4 mb-4">
                                            <i class="fas fa-shopping-cart me-2"></i> Add To Cart
                                        </a>
                                        <div class="d-flex justify-content-between align-items-center">
                                            <div class="d-flex">
                                                <i class="fas fa-star text-primary"></i>
                                                <i class="fas fa-star text-primary"></i>
                                                <i class="fas fa-star text-primary"></i>
                                                <i class="fas fa-star text-primary"></i>
                                                <i class="fas fa-star"></i>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }
                        
                    </div>
                </div>
                <div id="tab-2" class="tab-pane fade show p-0">
                    <div class="row g-4">
                        @foreach (var item in (NewProducts ?? Enumerable.Empty<Product>()).Take(4))
                        {
                            <div class="col-md-6 col-lg-4 col-xl-3">
                                <div class="product-item rounded wow fadeInUp" data-wow-delay="0.1s">
                                    <div class="product-item-inner border rounded">
                                        <div class="product-item-inner-item">
                                            <img src="@item.MainImage" class="img-fluid w-100 rounded-top" alt="">
                                            @if (item.CreatedAt >= DateTime.Now.AddMonths(-1))
                                            {
                                                <div class="product-new">New</div>
                                            }
                                            <div class="product-details">
                                                <a href="/SingleProduct/@item.Id"><i class="fa fa-eye fa-1x"></i></a>
                                            </div>
                                        </div>
                                        <div class="text-center rounded-bottom p-4">
                                            <a class="d-block mb-2">Camera</a>
                                            <a href="/SingleProduct/@item.Id" class="d-block h4">@item.ProductName</a>
                                            <del class="me-2 fs-5">@String.Format(new CultureInfo("vi-VN"), "{0:C0}", item.OriginalPrice)</del>
                                            <span class="text-primary fs-5">@String.Format(new CultureInfo("vi-VN"), "{0:C0}", item.SellingPrice)</span>
                                        </div>
                                    </div>
                                    <div class="product-item-add border border-top-0 rounded-bottom  text-center p-4 pt-0">
                                        <a onclick="addToCart(@item.Id)"
                                           class="btn btn-primary border-secondary rounded-pill py-2 px-4 mb-4">
                                            <i class="fas fa-shopping-cart me-2"></i> Add To Cart
                                        </a>
                                        <div class="d-flex justify-content-between align-items-center">
                                            <div class="d-flex">
                                                <i class="fas fa-star text-primary"></i>
                                                <i class="fas fa-star text-primary"></i>
                                                <i class="fas fa-star text-primary"></i>
                                                <i class="fas fa-star text-primary"></i>
                                                <i class="fas fa-star"></i>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }
                    </div>
                </div>
                <div id="tab-3" class="tab-pane fade show p-0">
                    <div class="row g-4">
                        @foreach (var item in (BestSeller ?? Enumerable.Empty<Product>()).Take(4))
                        {
                            <div class="col-md-6 col-lg-4 col-xl-3">
                                <div class="product-item rounded wow fadeInUp" data-wow-delay="0.1s">
                                    <div class="product-item-inner border rounded">
                                        <div class="product-item-inner-item">
                                            <img src="@item.MainImage" class="img-fluid w-100 rounded-top" alt="">
                                            @if (item.CreatedAt >= DateTime.Now.AddMonths(-1))
                                            {
                                                <div class="product-new">New</div>
                                            }
                                            <div class="product-details">
                                                <a href="/SingleProduct/@item.Id"><i class="fa fa-eye fa-1x"></i></a>
                                            </div>
                                        </div>
                                        <div class="text-center rounded-bottom p-4">
                                            <a class="d-block mb-2">Camera</a>
                                            <a href="/SingleProduct/@item.Id" class="d-block h4">@item.ProductName</a>
                                            <del class="me-2 fs-5">@String.Format(new CultureInfo("vi-VN"), "{0:C0}", item.OriginalPrice)</del>
                                            <span class="text-primary fs-5">@String.Format(new CultureInfo("vi-VN"), "{0:C0}", item.SellingPrice)</span>
                                        </div>
                                    </div>
                                    <div class="product-item-add border border-top-0 rounded-bottom  text-center p-4 pt-0">
                                        <a onclick="addToCart(@item.Id)"
                                           class="btn btn-primary border-secondary rounded-pill py-2 px-4 mb-4">
                                            <i class="fas fa-shopping-cart me-2"></i> Add To Cart
                                        </a>
                                        <div class="d-flex justify-content-between align-items-center">
                                            <div class="d-flex">
                                                <i class="fas fa-star text-primary"></i>
                                                <i class="fas fa-star text-primary"></i>
                                                <i class="fas fa-star text-primary"></i>
                                                <i class="fas fa-star text-primary"></i>
                                                <i class="fas fa-star"></i>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Our Products End -->
